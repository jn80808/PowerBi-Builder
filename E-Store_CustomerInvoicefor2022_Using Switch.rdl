        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
	 Customer		
	,Country		
	,Region	
	,Segment	
	,Salesperson
	,Emp_ID
	,Order_ID	
	,Order_Date
	,Required_Date
	,Shipped_Date
	,Shipper
	,Product_Name
	,Unit_Price
	,Quantity
	,Subtotal
